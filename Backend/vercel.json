{
    "version": 2,
    "builds": [
        {
            "src": "Backend.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Backend.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
} 